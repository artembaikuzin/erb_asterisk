[operators]
strategy = rrmemory
joinempty = yes
musicclass = queue
<% 15.times.each do |i| %>
<% group = 100 + i %>
<% op = 400 + i %>
member => Local/<%= "#{group}#{op}" %>@queue-dial-control,0,,PJSIP/<%= op %>
<% end %>
ringinuse = no
announce-frequency = 30
announce-holdtime = no
retry = 0

<%= include_to 'queues.conf.includes' %>
<% include_to 'queues.conf.includes' %>
